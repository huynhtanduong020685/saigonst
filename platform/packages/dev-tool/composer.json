{
    "name": "botble/dev-tool",
    "description": "Dev tools package for Botble CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Botble\\DevTool\\": "src"
        }
    },
    "require": {
        "jakub-onderka/php-parallel-lint": "dev-master",
        "squizlabs/php_codesniffer": "^3.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "phpmd/phpmd": "^2.8",
        "doctrine/dbal": "^2.9",
        "botble/git-commit-checker": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Botble\\DevTool\\Providers\\DevToolServiceProvider"
            ]
        }
    }
}
